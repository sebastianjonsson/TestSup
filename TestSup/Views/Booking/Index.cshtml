@model TestSup.Controllers.BookingIndexViewModel


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BookingSystemID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserMobile)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Subject)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Endtime)
        </th>

    </tr>
    @foreach (var item in Model.Bookings)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BookingSys.SystemName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserMobile)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Subject)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Endtime)
        </td>
        <td>
            @Html.ActionLink("Ta bort bokning", "Delete", new { id = item.Id })
        </td>
    </tr>
    }
</table>


